{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myfreeADF"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"CneosDataGenerator_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'CneosDataGenerator'"
		},
		"CneosSQLDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CneosSQLDatabase'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myazurefreetier.dfs.core.windows.net/"
		},
		"AzureKeyVaultFree_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mykeyvaultfree.vault.azure.net/"
		},
		"CneosDataGenerator_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0128-115905-5vkbhijl"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "myfreeADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"CneosDataGenerator_accessToken": {
						"value": "[parameters('CneosDataGenerator_accessToken')]"
					},
					"CneosSQLDatabase_connectionString": {
						"value": "[parameters('CneosSQLDatabase_connectionString')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"AzureKeyVaultFree_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultFree_properties_typeProperties_baseUrl')]"
					},
					"CneosDataGenerator_properties_typeProperties_existingClusterId": {
						"value": "[parameters('CneosDataGenerator_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}